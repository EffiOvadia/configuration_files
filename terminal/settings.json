  "firstWindowPreference": "persistedWindowLayout",
  "focusFollowMouse": false,
  "initialCols": 135,
  "initialPosition": "300,100",
  "initialRows": 30,
  "language": "en-US",
  "largePasteWarning": true,
  "launchMode": "default",
  "multiLinePasteWarning": false,
